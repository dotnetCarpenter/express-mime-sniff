{
  "name": "express-mime-sniff",
  "version": "1.0.0",
  "description": "express-mime-sniff is a middleware for express, setting HTTP `Content-Type` headers for files by utilizing a *nix `file` program, available on your system.",
  "types": "index.d.ts",
  "files": [
    "src/**/*",
    "index.js",
    "index.mjs"
  ],
  "type": "commonjs",
  "main": "index.js",
  "exports": {
    "import": "./index.mjs",
    "require": "./index.js"
  },
  "scripts": {
    "test": "jasmine",
    "test:watch": "watch -c npm test -- --color",
    "test:unit": "jasmine spec/unit-test/*.js",
    "test:functional": "jasmine spec/functional-test/*.js",
    "test:integration": "jasmine spec/integration-test/*.mjs",
    "benchmark": "node ./benchmark/server.mjs & (cd benchmark && sleep 2 && ./express-only.sh && ./mimesniff.sh && kill $(lsof -ti :3100))"
  },
  "keywords": [],
  "author": "",
  "license": "BSD-2-Clause",
  "peerDependencies": {
    "express": "^4.19.2"
  },
  "dependencies": {
    "sanctuary": "^3.1.0"
  },
  "devDependencies": {
    "express": "^4.19.2",
    "heapdump": "^0.3.15",
    "jasmine": "^5.1.0"
  }
}
