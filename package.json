  "peerDependencies": {
    "express": "^4.17.1"
  },
  "dependencies": {
    "sanctuary": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^15.6.1",
    "@types/sanctuary": "^3.0.3",
    "express": "^4.17.1",
    "jasmine": "^3.7.0"
  }
}
