{
  "name": "express-mime-sniff",
  "version": "0.3.0",
  "description": "express-mime-sniff is a middleware for express, setting HTTP `Content-Type` headers for files by utilizing a *nix `file` program, available on your system.",
  "types": "index.d.ts",
  "files": [
    "src/**/*",
    "index.mjs",
    "cjs/**/*",
    "index.cjs"
  ],
  "type": "module",
  "exports": {
    "import": "./index.mjs",
    "require": "./index.cjs"
  },
  "scripts": {
    "test": "jasmine",
    "test:watch": "watch -c npm test -- --color",
    "build": "tsc -p tsconfig.cjs.json"
  },
  "keywords": [],
  "author": "",
  "license": "BSD-2-Clause",
  "peerDependencies": {
    "express": "^4.17.1"
  },
  "dependencies": {
    "sanctuary": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "^15.6.1",
    "@types/sanctuary": "^3.0.3",
    "express": "^4.17.1",
    "jasmine": "^3.7.0",
    "typescript": "^4.2.4"
  }
}
